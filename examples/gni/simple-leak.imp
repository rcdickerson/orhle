// A program with a straightforward leak of high-security state.

expected: invalid;

forall: leak[1], leak[2];
exists: leak[3];

pre: (and
       (= leak!1!high  leak!3!high)
       (= leak!1!low   leak!2!low)
       (= leak!2!low   leak!3!low));
post:  (= leak!2!ret   leak!3!ret);

fun leak(high, low) {
  ret := high + low;
  return ret;
}