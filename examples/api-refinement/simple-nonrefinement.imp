// A program together with an invalid refinement.

forall: refinement;
exists: original;

post: (= original!x refinement!x);

expected: invalid;

prog original():
  x := call randInt()
         @templateVars { n }
         @pre  { (and (>= n 0) (< n 20)) }
         @post { (= x n) };
endp

prog refinement():
  x := call randInt()
         @pre  { true }
         @post { (and (>= x 0) (< x 25)) };
endp