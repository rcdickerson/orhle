
*******************************************
****               ORHLE               ****
****     The Oracular RHLE Verifier    ****
*******************************************

:: Executions
  nonleak[1] (forall)
  nonleak[2] (forall)
  nonleak[3] (exists)

:: Universal Programs
[ SSeq
    [ SAsgn nonleak !
        1 ! x (AAdd (AVar nonleak ! 1 ! low) (AVar nonleak ! 1 ! high))
    , SAsgn nonleak !
        1 ! ret (ASub (AVar nonleak ! 1 ! x) (AVar nonleak ! 1 ! high))
    , SAsgn nonleak ! 1 ! retVal (AVar nonleak ! 1 ! ret)
    ]
, SSeq
    [ SAsgn nonleak !
        2 ! x (AAdd (AVar nonleak ! 2 ! low) (AVar nonleak ! 2 ! high))
    , SAsgn nonleak !
        2 ! ret (ASub (AVar nonleak ! 2 ! x) (AVar nonleak ! 2 ! high))
    , SAsgn nonleak ! 2 ! retVal (AVar nonleak ! 2 ! ret)
    ]
]

:: Existential Programs
[ SSeq
    [ SAsgn nonleak !
        3 ! x (AAdd (AVar nonleak ! 3 ! low) (AVar nonleak ! 3 ! high))
    , SAsgn nonleak !
        3 ! ret (ASub (AVar nonleak ! 3 ! x) (AVar nonleak ! 3 ! high))
    , SAsgn nonleak ! 3 ! retVal (AVar nonleak ! 3 ! ret)
    ]
]

:: Universal Specifications
fromList []

:: Existential Specifications
fromList []

:: Precondition
And
  [ Eq (Var nonleak ! 1 ! high) (Var nonleak ! 3 ! high)
  , Eq (Var nonleak ! 1 ! low) (Var nonleak ! 2 ! low)
  , Eq (Var nonleak ! 2 ! low) (Var nonleak ! 3 ! low)
  ]

:: Postcondition
Eq (Var nonleak ! 2 ! ret) (Var nonleak ! 3 ! ret)

[send->] (set-option :print-success true )
[<-recv] success
[send->] (set-option :produce-models true )
[<-recv] success
[send->] (declare-const nonleak!1!high Int)
[<-recv] success
[send->] (declare-const nonleak!1!low Int)
[<-recv] success
[send->] (declare-const nonleak!2!high Int)
[<-recv] success
[send->] (declare-const nonleak!2!low Int)
[<-recv] success
[send->] (declare-const nonleak!3!high Int)
[<-recv] success
[send->] (declare-const nonleak!3!low Int)
[<-recv] success
[send->] (assert (not (=> (and (= nonleak!1!high nonleak!3!high)
              (= nonleak!1!low nonleak!2!low) (= nonleak!2!low nonleak!3!low))
         (= (- (+ nonleak!2!low nonleak!2!high) nonleak!2!high)
            (- (+ nonleak!3!low nonleak!3!high) nonleak!3!high)))) )
[<-recv] success
[send->] (check-sat )
[<-recv] unsat
Valid.

