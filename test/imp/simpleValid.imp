expected: valid;

forall: A;
exists: E;

pre: true;
post: (= A!y E!y);

especs:
 randOdd() {
   choiceVars: n;
   pre: (= (mod n 2) 1);
   post: (= ret! n);
 }

prog A():
  x := 3;
  if x == 3 then
    y := 3;
  else
    y := 300;
  end
endp

prog E():
  x := call randOdd();
  if x == 3 then
    y := 3;
  else
    y := 300;
  end
endp